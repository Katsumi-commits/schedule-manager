{
  "name": "ai-developer",
  "version": "0.1.0",
  "bin": {
    "ai-developer": "bin/ai-developer.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --coverage",
    "test:e2e": "playwright test",
    "test:integration": "node tests/integration.test.js",
    "test:report": "node scripts/generate-test-report.js",
    "test:comprehensive": "node scripts/generate-comprehensive-report.js",
    "lint": "echo 'Lint check placeholder'",
