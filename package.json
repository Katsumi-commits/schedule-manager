    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.1100.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "@playwright/test": "^1.40.0",
    "exceljs": "^4.4.0",
    "axios": "^1.6.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.232.1",
    "constructs": "^10.0.0"
  }
}
